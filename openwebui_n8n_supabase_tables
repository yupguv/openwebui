create table
  public.users (
    id bigint generated by default as identity not null,
    created_at timestamp with time zone not null default now(),
    email text null,
    name text null,
    openweb_thread_id text null,
    user_openweb_id text null,
    current_execution_id numeric null,
    constraint users_pkey primary key (id)
  ) tablespace pg_default;

create table
  public.responses (
    id bigint generated by default as identity not null,
    created_at timestamp with time zone not null default now(),
    response text null,
    openwebui_id text null,
    role text null,
    thread_id text null,
    retrieved boolean null default false,
    seconds numeric null default '0'::numeric,
    execution_id numeric null,
    constraint responses_pkey primary key (id)
  ) tablespace pg_default;
